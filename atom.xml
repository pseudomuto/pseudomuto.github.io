---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Your Website</title>
  <link href="http://pseudomuto.com/"/>
  <link type="application/atom+xml" rel="self" href="http://pseudomuto.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://pseudomuto.com/</id>
  <author>
    <name>David Muto</name>
    <email>david.muto@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://pseudomuto.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://pseudomuto.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>David Muto</name>
      <uri>http://pseudomuto.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>